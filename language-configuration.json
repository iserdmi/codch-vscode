{
  "comments": {
    "lineComment": "#"
  },
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\[(config|strategy|defaultTask|knowledge|context|output|task)\\]",
      "end": "^\\s*\\[/\\1\\]"
    }
  }
}
