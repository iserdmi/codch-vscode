{
  "$schema": "http://json.schemastore.org/tmLanguage",
  "name": "CODCH",
  "scopeName": "source.codch",
  "patterns": [
    {
      "name": "comment.line.codch",
      "match": "#.*$"
    },
    {
      "name": "keyword.control.codch",
      "match": "\\[(config|strategy|defaultTask|context|output|task)\\]\\n"
    },
    {
      "name": "keyword.control.codch",
      "match": "\\[/(config|strategy|defaultTask|context|output|task)\\]?"
    },
    {
      "begin": "\\[(config|strategy|defaultTask|context|output|task)\\]",
      "end": "\\[/\\1\\]",
      "name": "meta.section.codch",
      "patterns": [
        {
          "include": "#comment"
        }
      ]
    }
  ],
  "repository": {
    "comment": {
      "name": "comment.line.codch",
      "match": "#.*$"
    }
  }
}
